# Copy this file to .env and fill in your values.
# These variables must also be set in the Netlify UI under Site Settings â†’ Environment Variables.

# Database connection string (e.g. MongoDB Atlas, PostgreSQL)
DATABASE_URL=mongodb+srv://<user>:<password>@cluster.mongodb.net/tiktok

# Secret used to sign JWT tokens (generate a strong random string)
JWT_SECRET=change-me-to-a-long-random-string

# Cloud object storage (e.g. AWS S3, Cloudflare R2, Backblaze B2)
STORAGE_BUCKET=your-bucket-name
STORAGE_KEY_ID=your-key-id
STORAGE_SECRET=your-storage-secret
STORAGE_REGION=us-east-1

# Vite public variables (prefix with VITE_ to expose to the browser)
VITE_API_BASE_URL=/.netlify/functions
